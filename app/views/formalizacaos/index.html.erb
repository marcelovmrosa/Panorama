<p id="notice"><%= notice %></p>

<h1>Listing Formalizacaos</h1>

<table>
  <thead>
    <tr>
      <th>Tema</th>
      <th>Justificativa</th>
      <th>Validacao</th>
      <th>Motivo</th>
      <th>Data</th>
      <th>Aluno</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @formalizacaos.each do |formalizacao| %>
      <tr>
        <td><%= formalizacao.tema %></td>
        <td><%= formalizacao.justificativa %></td>
        <td><%= formalizacao.validacao %></td>
        <td><%= formalizacao.motivo %></td>
        <td><%= formalizacao.data %></td>
        <td><%= formalizacao.aluno %></td>
        <td><%= link_to 'Show', formalizacao %></td>
        <td><%= link_to 'Edit', edit_formalizacao_path(formalizacao) %></td>
        <td><%= link_to 'Destroy', formalizacao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Formalizacao', new_formalizacao_path %>
